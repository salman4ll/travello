<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bg">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/judul"
            >
            <TextView android:id="@+id/hei__welcome_to"
                android:textSize="24sp"
                android:textColor="@color/hei__welcome_to_color"
                android:layout_marginTop="110dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/hei__welcome_to_string"
                android:layout_centerHorizontal="true"
                />
            <TextView android:id="@+id/travello_"
                android:textSize="36sp"
                android:textColor="@color/hei__welcome_to_color"
                android:layout_marginTop="146dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/travello__string"
                android:layout_centerHorizontal="true"
                />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/judul"
            >
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:background="@drawable/button"
                android:layout_marginTop="693dp"
                android:layout_centerHorizontal="true"
                />
        </RelativeLayout>
    </RelativeLayout>



</androidx.constraintlayout.widget.ConstraintLayout>